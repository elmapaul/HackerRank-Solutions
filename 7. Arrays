using System;
using System.Collections.Generic;
using System.IO;
using System.Linq;
class Solution {

    static void Main(String[] args) {
        int n = Convert.ToInt32(Console.ReadLine());
        string[] arr_temp = Console.ReadLine().Split(' ');
        int[] arr = Array.ConvertAll(arr_temp,Int32.Parse);
        string s ="";
        
        
        for (int i = n - 1; i >= 0; i--)
        {
            s += arr_temp[i] + " ";
        }
        
        Console.WriteLine(s);
    }
}
